{
  "name": "plantcare-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:prod": "ng serve --configuration production",
    "start:big": "node --max-old-space-size=12288 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json",
    "start:max": "node --max-old-space-size=16384 --max-semi-space-size=512 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json",
    "build": "node --max-old-space-size=12288 ./node_modules/@angular/cli/bin/ng build",
    "build:prod": "node --max-old-space-size=16384 ./node_modules/@angular/cli/bin/ng build --configuration production",
    "build:analyze": "node --max-old-space-size=16384 ./node_modules/@angular/cli/bin/ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/fe_code/browser/stats.json",
    "watch": "node --max-old-space-size=12288 ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:fe_code": "node dist/fe_code/server/server.mjs",
    "docker:build": "docker build -t plantcare-frontend .",
    "docker:run": "docker run -p 80:80 plantcare-frontend",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.1",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.1",
    "@angular/compiler": "^20.0.1",
    "@angular/core": "^20.0.1",
    "@angular/forms": "^20.0.1",
    "@angular/material": "^20.0.1",
    "@angular/platform-browser": "^20.0.1",
    "@angular/platform-server": "^20.0.1",
    "@angular/router": "^20.0.1",
    "@angular/ssr": "^20.0.5",
    "@stomp/stompjs": "^7.1.1",
    "chart.js": "^4.5.0",
    "express": "^5.1.0",
    "jwt-decode": "^4.0.0",
    "ng2-charts": "^5.0.4",
    "ngx-cookie-service": "^20.0.1",
    "ngx-image-cropper": "^9.1.5",
    "rxjs": "~7.8.0",
    "sockjs-client": "^1.6.1",
    "tslib": "^2.8.1",
    "vanilla-tilt": "^1.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.1",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/sockjs-client": "^1.5.4",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
